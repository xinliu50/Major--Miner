  "dependencies": {
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.2",
    "bootstrap": "^4.3.1",
    "chai": "^4.2.0",
    "firebase": "^5.10.1",
    "firebase-admin": "^8.9.2",
    "firebase-functions": "^3.3.0",
